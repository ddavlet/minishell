	"version": "0.2.0",
	"configurations": [
		{
			"name": "parsing_debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/parsing.out",
			// "args": ["echo *USER > USER  test1 test2 'test_test'| echo test2 ||  echo test3 && echo test4"],
			// "args": ["echo *USER >>> USER ||| <<test1 >>test2 'test_test'"],
			// "args": [">>test'&|&'"],
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "Parsing",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "parsing_Mac",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/parsing.out",
			// "args": ["'e'ch'o' \"$TEST\" | echo test2 || echo test3 && echo test4"],
			"args": ["echo <infile >outgfile test |"],
			// "args": ["echo"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb",
			"preLaunchTask": "Parsing"
		},
		{
			"name": "builins_debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/builtin.out",
			// "args": ["echo *USER > USER  test1 test2 'test_test'| echo test2 ||  echo test3 && echo test4"],
			// "args": ["echo *USER >>> USER ||| <<test1 >>test2 'test_test'"],
			// "args": [">>test'&|&'"],
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "Builtins",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		},
		{
			"name": "wildcard_debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/wildcard.out",
			"args": ["file1", "file2", "test1", "\*st", "te\*", "te\*t"],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "Wildcard",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}
